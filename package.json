{
  "name": "valido",
  "version": "2.0.16",
  "description": "Check and validation library",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha",
    "deploy-patch": "npm version patch && npm publish && git add . && git commit -m",
    "deploy-minor": "npm version minor && npm publish && git add . && git commit -m",
    "deploy-major": "npm version major && npm publish && git add . && git commit -m",
    "transpile": "babel src --retain-lines --out-dir dist",
    "prepublish": "npm test && npm run transpile && npm run generate-docs",
    "generate-docs": "doxdox './src/*.js' --layout markdown --output README.md"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moooji/valido.git"
  },
  "keywords": [
    "validation",
    "check",
    "validate",
    "predicate"
  ],
  "author": "Steffen Str√§tz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moooji/valido/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "doxdox": "^2.0.3",
    "eslint": "^4.9.0",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "mocha-eslint": "^4.1.0"
  }
}
